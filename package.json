{
  "name": "bunny-rest",
  "version": "1.0.6",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:prepare": "docker-compose -f docker-compose.dev.yml up -d --build",
    "dev": "cross-env NODE_ENV=development ts-node-dev --ignore-watch='src/openapi/' --respawn --transpile-only src/index.ts",
    "dev:nodemon": "cross-env NODE_ENV=development nodemon src/index.ts",
    "dev:unprepare": "docker-compose -f docker-compose.dev.yml down",
    "dev:compose": "DOCKER_DEV=yes ts-node-dev --ignore-watch='src/openapi/' --respawn --transpile-only src/index.ts",
    "test": "cross-env NODE_ENV=test node dist/index.js",
    "build": "cp -R certs dist/ && cp -R src/openapi dist/src && cp .env dist && yarn tsc",
    "build:compose": "cp -R certs dist/ && cp -R src/openapi dist/src && yarn tsc",
    "serve": "node dist/src/index.js",
    "serve:compose": "DOCKER_DEV=yes node dist/src/index.js",
    "dev:all-in-docker": "docker-compose -f docker-compose.dev-all.yml up -d --build",
    "undev:all-in-docker": "docker-compose -f docker-compose.dev-all.yml down",
    "deploy:test": "docker-compose -f docker-compose.test.yml up -d --build",
    "undeploy:test": "docker-compose -f docker-compose.test.yml down",
    "deploy": "docker-compose -f docker-compose.yml up -d --build",
    "undeploy": "docker-compose -f docker-compose.yml down",
    "detect:circular-deps": "dependency-cruiser --exclude '^src/(openapi|libs)/' src"
  },
  "dependencies": {
    "apollo-server": "^3.12.0",
    "bcrypt": "^5.1.0",
    "config": "^3.3.9",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "graphql": "^16.7.1",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^5.7.0",
    "nanoid": "^4.0.2",
    "nexus": "^1.3.0",
    "openapi3-ts": "^4.1.2",
    "pg": "^8.11.1",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.1",
    "prom-client": "^14.2.0",
    "redis": "^4.6.7",
    "response-time": "^2.3.2",
    "yaml": "^2.3.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/connect-redis": "^0.0.20",
    "@types/cors": "^2.8.13",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.17",
    "@types/i18n": "^0.13.6",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.10.2",
    "@types/pino": "^7.0.5",
    "@types/response-time": "^2.3.5",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "dependency-cruiser": "^13.1.1",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^4.9.5"
  }
}
